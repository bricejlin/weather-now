{
  "name": "weather-now",
  "version": "0.0.1",
  "description": "Real-time weather reporting for your device's exact location / time",
  "main": "index.js",
  "scripts": {
    "start": "node src/server.js",
    "test": "mocha",
    "test:watch": "npm test -- -w",
    "test-cov": "istanbul cover _mocha  --report lcovonly -- --compilers js:babel-register && codecov",
    "deploy": "NODE_ENV=production webpack -p --config webpack.prod.config.js",
    "postinstall": "npm run deploy"
  },
  "engines": {
    "node": "5.3.0"
  },
  "keywords": [
    "weather",
    "realtime"
  ],
  "author": "Brice Lin <bricejlin@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "codecov": "^1.0.1",
    "expect": "^1.14.0",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^2.4.5"
  },
  "dependencies": {
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.3",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.0",
    "babel-register": "^6.5.2",
    "express": "^4.13.4",
    "express-request-proxy": "^1.2.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.0",
    "redux": "^3.3.1",
    "redux-logger": "^2.6.0",
    "redux-thunk": "^1.0.3",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.7.1"
  }
}
